<?xml version="1.0" ?>
<gazebo version="1.2">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Hokuyo sensor -->
    <include>
      <uri>model://hokuyo</uri>
      <pose>-4 0 0 0 0 0</pose>
    </include>

    <!-- Bookshelf -->
    <include>
      <uri>model://bookshelf</uri>
      <pose>-2 4 0 0 0 0</pose>
    </include>

    <!-- A pair of spheres -->
    <model name='model_spheres'>
      <pose>1 1 1.5 0 0 0</pose>
      <link name="sphere1">
        <inertial>
          <mass>10</mass>
          <pose>0 0 0 0 0 0</pose>
          <inertia>
            <ixx>2.5</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>2.5</iyy>
            <iyz>0</iyz>
            <izz>2.5</izz>
          </inertia>
        </inertial>
        <visual name="visual">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <sphere><radius>0.5</radius></sphere>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
        <collision name="collision">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <sphere><radius>0.5</radius></sphere>
          </geometry>
        </collision>
        <sensor name="sensor_contact" type="contact">
          <always_on>True</always_on>
          <update_rate>1</update_rate>
          <contact>
            <collision>collision</collision>
          </contact>
        </sensor>
      </link>

      <link name="sphere10">
        <inertial>
          <mass>10</mass>
          <pose>0 2 0 0 0 0</pose>
          <inertia>
            <ixx>2.5</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>2.5</iyy>
            <iyz>0</iyz>
            <izz>2.5</izz>
          </inertia>
        </inertial>
        <visual name="visual10">
          <pose>0 2 0 0 0 0</pose>
          <geometry>
            <sphere><radius>0.5</radius></sphere>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
        <collision name="collision10">
          <pose>0 2 0 0 0 0</pose>
          <geometry>
            <sphere><radius>0.5</radius></sphere>
          </geometry>
        </collision>
        <sensor name="sensor_contact" type="contact">
          <always_on>True</always_on>
          <update_rate>1</update_rate>
          <contact>
            <collision>collision10</collision>
          </contact>
        </sensor>
      </link>

      <link name="sphere100">
        <pose>0 -2 0 0 0 0</pose>
        <inertial>
          <mass>100</mass>
          <inertia>
            <ixx>25</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>25</iyy>
            <iyz>0</iyz>
            <izz>25</izz>
          </inertia>
        </inertial>
        <visual name="visual100">
          <geometry>
            <sphere><radius>0.5</radius></sphere>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
        <collision name="collision100">
          <geometry>
            <sphere><radius>0.5</radius></sphere>
          </geometry>
        </collision>
        <sensor name="sensor_contact" type="contact">
          <always_on>True</always_on>
          <update_rate>1</update_rate>
          <contact>
            <collision>collision100</collision>
          </contact>
        </sensor>
      </link>
 
    </model>
  </world>
</gazebo>
